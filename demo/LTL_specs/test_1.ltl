CONTRACT C1:
    input integer (0 : 3)
        variable x;
    input integer (0 : 3) variable y;
    output integer (0 : 7) variable z;

    Assumptions:
        G(y != 0);
    Guarantees:
        G(z = x' / y);

CONTRACT C2:
    input integer (0 : 7) variable u;
    output integer (0 : 7) variable x;

    Assumptions:
        true;
    Guarantees:
        G(x > u);

CONTRACT C2_primed:
    input integer (0 : 7) variable v;
    output integer (-7 : 0) variable y;

    Assumptions:
        true;
    Guarantees:
        G(y = 0-v);